(assume a1 (= b false))
(assume a2 (= b true))
(assume a3 (or b true false))
(assume a4 (= b true))
(step t1 (cl (= true false)) :rule trans :premises (a1 a2))
(step t2 (cl (not true) false) :rule equiv1 :premises (t1))
(step t3 (cl (= (not (not b)) b)) :rule rare_rewrite :premises (a4) :args ("bool-double-not-elim" (rare-list b)))
