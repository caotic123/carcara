
(program test ((x Int) (y Int))
  :signature (Int Int) Bool
  (
  ((test 0 0) true)
  ((test 0 1) true)
  ((test x y) false)
  )
)

(declare-rare-rule or_eval ((xs Bool :list) (ys Bool :list)) 
  :args (ys xs)
  :conclusion (= (or xs true ys) true)
)

(declare-rare-rule and_is_true ((x Bool :list)) 
  :args (x)
  :premises ((= (test 1 2) true))
  :conclusion (= (and x) x)
  )